/**
 * 
 */
package com.spring.coupon.entity;

import java.math.BigDecimal;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

/**
 * @author Suresh Sadanala
 *
 */
@Entity
public class Coupon {

	private static final long serialVersionUID = 3313179101618816943L;

	@Id
	@GeneratedValue(strategy = GenerationType.IDENTITY)
	private Long id;
	private String code;
	private BigDecimal dicsount;
	private String expnDate;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public BigDecimal getDicsount() {
		return dicsount;
	}

	public void setDicsount(BigDecimal dicsount) {
		this.dicsount = dicsount;
	}

	public String getExpnDate() {
		return expnDate;
	}

	public void setExpnDate(String expnDate) {
		this.expnDate = expnDate;
	}

}
